<div class="container" style="height:800px;">

<div class="category_list">


<div class="table-users">
   <div class="header">List Of Category</div>
   
   <table cellspacing="0">
      <tr>
         <th>Id</th>
         <th>Name</th>
         <th>Image</th>
         <th>Edit</th>
         <th>Delete</th>
      </tr>

      <tr <% @categories.each do |category| %>>
        <td> <%= category.id %></td>
        <td> <%= category.name %></td>
        <td> <%= image_tag category.image, size: "120x60", alt: "Edit Entry" %></td>
        
        <td> <%= link_to "Edit", edit_category_path(category) %> </td>
        <td><%= button_to 'Delete', category_path(category), method: 'delete', data: {confirm: "Are you sure?"}  %></td>       
       </tr <% end %>>
   </table>
   
</div>
