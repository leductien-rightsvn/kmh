<%= simple_form_for [:console, @category] do |f| %>
    <%= f.input :name %>
    <%= f.file_field :image %> 
    <%= f.submit %>
    <br>
    <%= link_to t("view.operation.new"), new_console_category_path %>
    <%= link_to t("categories.form.listCategory_title"), console_categories_path %>
<% end %>

