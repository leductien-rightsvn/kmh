<div class="container" >
  <div class="category_form">
    <h2><%= t(".title") %></h2>
    <%= render "form" %>
    <%= link_to t("view.operation.new"), new_console_category_path %>
    <%= link_to t("view.operation.list"), console_categories_path %>
  </div>
  <div class="category_image">
    <% if @category.image.present? %>
      <%= image_tag(@category.image, size: "390x390", alt: "Edit Entry", class: "image_category") %>
    <% end %>
  </div>
</div>  
